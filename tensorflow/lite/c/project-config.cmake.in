# Configure @TENSORFLOW_LITE_C_NAME@
#
# Set
#  @TENSORFLOW_LITE_C_NAME@_FOUND = 1
#  @TENSORFLOW_LITE_C_NAME@_INCLUDE_DIRS = /usr/local/include
#  @TENSORFLOW_LITE_C_NAME@_LIBRARY = @TENSORFLOW_LITE_C_NAME@
#  @TENSORFLOW_LITE_C_NAME@_LIBRARY_DIRS = /usr/local/lib
#  @TENSORFLOW_LITE_C_NAME@_BINARY_DIRS = /usr/local/bin
#  @TENSORFLOW_LITE_C_NAME@_VERSION = 1.4.1 (for example)

message (STATUS "Reading ${CMAKE_CURRENT_LIST_FILE}")
# @TENSORFLOW_LITE_C_NAME@_VERSION is set by version file
message (STATUS
  "@TENSORFLOW_LITE_C_NAME@ configuration, version ${@TENSORFLOW_LITE_C_NAME@_VERSION}")

# Tell the user project where to find our headers and libraries
get_filename_component (_DIR ${CMAKE_CURRENT_LIST_FILE} PATH)
get_filename_component (_ROOT "${_DIR}/@PROJECT_ROOT_DIR@" ABSOLUTE)
set (@TENSORFLOW_LITE_C_NAME@_INCLUDE_DIRS "${_ROOT}/include")
set (@TENSORFLOW_LITE_C_NAME@_LIBRARY_DIRS "${_ROOT}/lib")
set (@TENSORFLOW_LITE_C_NAME@_BINARY_DIRS "${_ROOT}/bin")

set ( @TENSORFLOW_LITE_C_NAME@_LIBRARY @TENSORFLOW_LITE_C_NAME@ )
set ( @TENSORFLOW_LITE_C_NAME@_SHARED_LIBS @TFLITE_C_BUILD_SHARED_LIBS@ )

message (STATUS "  include directory: \${@TENSORFLOW_LITE_C_NAME@_INCLUDE_DIRS}")

if(${@TENSORFLOW_LITE_C_NAME@_SHARED_LIBS})
  add_library( ${@TENSORFLOW_LITE_C_NAME@_LIBRARY} SHARED IMPORTED GLOBAL) 
else()
  add_library( ${@TENSORFLOW_LITE_C_NAME@_LIBRARY} STATIC IMPORTED GLOBAL)
endif()

if(WIN32)
    set(implib_suffix .lib)
    set(location ${@TENSORFLOW_LITE_C_NAME@_BINARY_DIRS}/@TENSORFLOW_LITE_C_PREFIX@${@TENSORFLOW_LITE_C_NAME@_LIBRARY}@TENSORFLOW_LITE_C_SUFFIX@)
else()
    set(implib_suffix @TENSORFLOW_LITE_C_SUFFIX@)
    set(location ${@TENSORFLOW_LITE_C_NAME@_LIBRARY_DIRS}/@TENSORFLOW_LITE_C_PREFIX@${@TENSORFLOW_LITE_C_NAME@_LIBRARY}${implib_suffix})
endif()

set_target_properties(${@TENSORFLOW_LITE_C_NAME@_LIBRARY} PROPERTIES                                     
  IMPORTED_LINK_INTERFACE_LANGUAGES "CXX"
  IMPORTED_LOCATION "${location}"
  IMPORTED_IMPLIB "${@TENSORFLOW_LITE_C_NAME@_LIBRARY_DIRS}/@TENSORFLOW_LITE_C_PREFIX@${@TENSORFLOW_LITE_C_NAME@_LIBRARY}${implib_suffix}"
  INTERFACE_INCLUDE_DIRECTORIES "${@TENSORFLOW_LITE_C_NAME@_INCLUDE_DIRS}"
)

unset (_ROOT)
unset (_DIR)

# For compatibility with FindGeoTIFF.cmake
set (@TENSORFLOW_LITE_C_NAME_UPPER@_FOUND 1)
set (@TENSORFLOW_LITE_C_NAME_UPPER@_LIBRARIES ${@TENSORFLOW_LITE_C_NAME@_LIBRARY})
set (@TENSORFLOW_LITE_C_NAME_UPPER@_INCLUDE_DIR ${@TENSORFLOW_LITE_C_NAME@_INCLUDE_DIRS})
set (@TENSORFLOW_LITE_C_NAME_UPPER@_LIBRARY ${@TENSORFLOW_LITE_C_NAME@_LIBRARY})
